default
partial alphanumeric_keys modifier_keys
xkb_symbols "emacs" {
    key <AD01> {
        type= "EMACS_KEYS",
            symbols[Group1]= [ q, Q, Home, Home ],
            actions[Group1]= [ NoAction(), NoAction(), RedirectKey(keycode=<HOME>,clearMods= Control), RedirectKey(keycode=<HOME>,mods= Shift,clearMods= Control) ]
    };
    key <AD03> {
        type= "EMACS_KEYS",
            symbols[Group1]= [ e, E, End, End ],
            actions[Group1]= [ NoAction(), NoAction(), RedirectKey(keycode=<END>,clearMods= Control), RedirectKey(keycode=<END>,mods= Shift,clearMods= Control) ]
    };
    key <AD10> {
        type= "EMACS_KEYS",
            symbols[Group1]= [ p, P, Up, Up ],
            actions[Group1]= [ NoAction(), NoAction(), RedirectKey(keycode=<UP>,clearMods= Control), RedirectKey(keycode=<UP>,mods= Shift,clearMods= Control) ]
    };
    key <AC04> {
        type= "EMACS_KEYS",
            repeat= true,
            symbols[Group1]= [ f, F, Right, Right ],
            actions[Group1]= [NoAction(), NoAction(), RedirectKey(keycode=<RGHT>, clearmods=Control), RedirectKey(keycode=<RGHT>, mods= Shift, clearmods= Control)]
    };
    key <AB05> {
        type= "EMACS_KEYS",
            symbols[Group1]= [ b, B, Left, Left ],
            actions[Group1]= [ NoAction(), NoAction(), RedirectKey(keycode=<LEFT>,clearMods= Control), RedirectKey(keycode=<LEFT>,mods= Shift,clearMods= Control) ]
    };
    key <AB06> {
        type= "EMACS_KEYS",
            symbols[Group1]= [ n, N, Down, Down ],
            actions[Group1]= [ NoAction(), NoAction(), RedirectKey(keycode=<DOWN>,clearMods= Control), RedirectKey(keycode=<DOWN>,mods= Shift,clearMods= Control) ]
    };
};
